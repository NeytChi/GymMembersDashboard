<Window x:Class="GymMembersDashboard.MainWindow"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GymMembersDashboard"
        mc:Ignorable="d"
        Title="MainWindow" Height="850" Width="1250">
    <Grid>
        <Button/>
        <lvc:PieChart Name="DGenders" Series="{Binding PieMemberGenders}" Margin="0,10,995,580"/>
        <lvc:CartesianChart Name="DWorkOut" Margin="251,10,498,580"/>
        <lvc:CartesianChart Name="DWorkOutFrequency" Margin="752,10,10,580" Series="{Binding PieMemberGenders}" />
        <lvc:CartesianChart Name="DExperienceLevel" Margin="10,259,749,298" Series="{Binding PieMemberGenders}" />
        <lvc:CartesianChart Name="DWeight" Margin="501,259,221,298" Series="{Binding PieMemberGenders}" />
        <lvc:PieChart x:Name="DSessionDuration" Series="{Binding PieMemberGenders}" Margin="1034,254,10,370"/>

    </Grid>
</Window>
